{
  "compilerOptions": {
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "ES2022"],
    "module": "ES2022",
    "moduleResolution": "Node10",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022"
  },
  "include": ["examples", "packages"]
}
