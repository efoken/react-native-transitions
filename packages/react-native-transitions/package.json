{
  "name": "react-native-transitions",
  "version": "0.0.6",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "react-native": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "license": "MIT",
  "dependencies": {
    "merge-refs": "^1.2.2",
    "react-transition-state": "^2.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.74",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": ">=0.71.4"
  },
  "scripts": {
    "build": "run -T tsup src/index.ts --dts --format esm,cjs",
    "prepack": "yarn build"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "react-native": "./src/index.ts",
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  }
}
